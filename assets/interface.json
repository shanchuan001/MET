{
    "controller": [
        {
            "name": "安卓端(模拟器选这个)",
            "type": "Adb"
        },
        {
            "name": "桌面端(没测试过)",
            "type": "Win32",
            "win32": {
                "method": "Search",
                "class_name": ".*",
                "window_name": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "B服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource/bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "自动主线扫荡",
            "entry": "主线扫荡开始",
            "option": [
                "扫荡关卡"
            ]
        },
        {
            "name": "自动助战任务(此任务会领取每日奖励,所以请放到主线扫荡后,自动多人战前)",
            "entry": "助战任务开始",
            "option": [
                "出战队伍属性",
                "出战队伍序号",
                "助战虚数体(请通过好友星标把要借的虚数体恒定在助战页面的前四位)",
                "是否无限刷助战币"
            ]
        },
        {
            "name": "自动多人战",
            "entry": "返回主页",
            "option": [
                "要打的关卡",
                "出战队伍属性",
                "出战队伍序号",
                "助战虚数体(请通过好友星标把要借的虚数体恒定在助战页面的前四位)",
                "是否允许他人加入房间"
            ]
        },
        {
            "name": "自动多人战-蹭车模式",
            "entry": "返回主页",
            "option": [
                "要打的关卡",
                "出战队伍属性",
                "出战队伍序号",
                "助战虚数体(请通过好友星标把要借的虚数体恒定在助战页面的前四位)",
                "每把时间大于多少更换房间",
                "连续多久未找到房间后退出此任务(继续下一任务,可以连续添加此任务来延长时间)"
            ]
        }
    ],
    "option": {
        "扫荡关卡": {
            "cases": [
                {
                    "name": "困难",
                    "pipeline_override": {}
                },
                {
                    "name": "极难",
                    "pipeline_override": {
                        "zx点击难度": {
                            "expected": "极难"
                        },
                        "zx识别结束": {
                            "expected": "第六章"
                        }
                    }
                }
            ]
        },
        "是否无限刷助战币": {
            "cases": [
                {
                    "name": "自动识别任务是否完成(包括每日和每周)",
                    "pipeline_override": {
                        "编队选择后的识别": {
                            "next": [
                                "自动战斗",
                                "出击_助战"
                            ]
                        },
                        "自动战斗": {
                            "next": "出击_助战"
                        },
                        "战斗确认": {
                            "next": "返回主页_助战1"
                        },
                        "超时跳车4": {
                            "next": "返回主页_助战1"
                        },
                        "等待进入": {
                            "next": "助战任务开始"
                        }
                    }
                },
                {
                    "name": "不识别,一直重复",
                    "pipeline_override": {
                        "编队选择后的识别": {
                            "next": [
                                "自动战斗",
                                "出击_助战"
                            ]
                        },
                        "自动战斗": {
                            "next": "出击_助战"
                        },
                        "助战任务开始": {
                            "next": "助战任务"
                        },
                        "战斗确认": {
                            "next": "返回主页_助战1"
                        },
                        "超时跳车4": {
                            "next": "返回主页_助战1"
                        },
                        "等待进入": {
                            "next": "助战任务开始"
                        }
                    }
                }
            ]
        },
        "每把时间大于多少更换房间": {
            "cases": [
                {
                    "name": "1分钟",
                    "pipeline_override": {
                        "准备": {
                            "next": "加入房间_准备"
                        },
                        "识别进入房间(next为等待战斗完成)": {
                            "timeout": 60000
                        },
                        "四级菜单": {
                            "next": "加入房间"
                        },
                        "自动战斗": {
                            "next": [
                                "准备"
                            ]
                        }
                    }
                },
                {
                    "name": "2分钟",
