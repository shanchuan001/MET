                        "识别进入房间(next为等待战斗完成)": {
                            "timeout": 120000
                        },
                        "四级菜单": {
                            "next": "加入房间"
                        },
                        "自动战斗": {
                            "next": [
                                "准备"
                            ]
                        }
                    }
                },
                {
                    "name": "3分钟",
                    "pipeline_override": {
                        "准备": {
                            "next": "加入房间_准备"
                        },
                        "识别进入房间(next为等待战斗完成)": {
                            "timeout": 180000
                        },
                        "四级菜单": {
                            "next": "加入房间"
                        },
                        "自动战斗": {
                            "next": [
                                "准备"
                            ]
                        }
                    }
                },
                {
                    "name": "4分钟",
                    "pipeline_override": {
                        "准备": {
                            "next": "加入房间_准备"
                        },
                        "识别进入房间(next为等待战斗完成)": {
                            "timeout": 240000
                        },
                        "四级菜单": {
                            "next": "加入房间"
                        },
                        "自动战斗": {
                            "next": [
                                "准备"
                            ]
                        }
                    }
                },
                {
                    "name": "5分钟",
                    "pipeline_override": {
                        "准备": {
                            "next": "加入房间_准备"
                        },
                        "识别进入房间(next为等待战斗完成)": {
                            "timeout": 300000
                        },
                        "四级菜单": {
                            "next": "加入房间"
                        },
                        "自动战斗": {
                            "next": [
                                "准备"
                            ]
                        }
                    }
                }
            ]
        },
        "连续多久未找到房间后退出此任务(继续下一任务,可以连续添加此任务来延长时间)": {
            "cases": [
                {
                    "name": "3分钟",
                    "pipeline_override": {
                        "未找到_计数2": {
                            "next": [
                                "停止"
                            ]
                        }
                    }
                },
                {
                    "name": "5分钟",
                    "pipeline_override": {
                        "未找到_计数4": {
                            "next": [
                                "停止"
                            ]
                        }
                    }
                },
                {
                    "name": "10分钟",
                    "pipeline_override": {
                        "未找到_计数9": {
                            "next": [
                                "停止"
                            ]
                        }
                    }
                },
                {
                    "name": "无限制",
                    "pipeline_override": {
                        "未找到_计数1": {
                            "next": [
                                "加入房间"
                            ]
                        }
                    }
                }
            ]
        },
